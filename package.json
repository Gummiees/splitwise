{
  "name": "angular-template",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint:es": "ng lint --fix",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint": "npm run lint:es && npm run lint:styles",
    "watch": "ng build --watch --configuration development",
    "build": "ng build",
    "buildpro": "ng build --configuration production",
    "buildpro:github": "ng build --configuration production --base-href https://gummiees.github.io/angular-template/",
    "commit": "npm run lint && git add . && git commit -m \"auto-commit\"",
    "push": "npm run commit && git push"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/fire": "^16.0.0",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "primeicons": "^6.0.1",
    "primeng": "^16.9.1",
    "rxjs": "^7.5.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.11",
    "@angular-eslint/builder": "16.3.1",
    "@angular-eslint/eslint-plugin": "16.3.1",
    "@angular-eslint/eslint-plugin-template": "16.3.1",
    "@angular-eslint/schematics": "16.3.1",
    "@angular-eslint/template-parser": "16.3.1",
    "@angular/cli": "~16.2.11",
    "@angular/compiler-cli": "^16.2.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "firebase-tools": "^11.15.0",
    "postcss": "^8.4.18",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "tailwindcss": "^3.2.1",
    "typescript": "~4.9.5"
  }
}
